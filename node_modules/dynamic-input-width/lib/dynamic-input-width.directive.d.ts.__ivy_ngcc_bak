import { ElementRef, OnDestroy, OnInit, Renderer2 } from '@angular/core';
export declare type DynamicWidthDirectiveParams = {
    minWidth: number;
    maxWidth: number;
};
export declare class DynamicWidthDirective implements OnInit, OnDestroy {
    readonly renderer: Renderer2;
    readonly elementRef: ElementRef;
    private unsubscribe;
    private element;
    params: DynamicWidthDirectiveParams;
    constructor(renderer: Renderer2, elementRef: ElementRef);
    ngOnInit(): void;
    ngOnDestroy(): void;
    triggerRecalculate(): void;
    private adjustWidth;
}
